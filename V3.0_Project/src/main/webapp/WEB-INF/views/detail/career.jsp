<script>

$(document).ready(function(){
	
	function scrollCheck() {
		var elId = "";
		$(".contents").each(function(key,val){
			if($(val).hasClass("on")){
				elId = $(val).prop("id");
			}
		});
		
	    var st = $("#"+elId+" .career_main").scrollTop(); 
	    var ih = Math.ceil($("#"+elId+" .career_main").innerHeight()) + 3;
	    var sh = $("#"+elId+" .career_main").prop("scrollHeight");
	    console.log(st);
	    console.log(ih);
	    console.log(sh);
	    if(st == 0 && st + ih < sh){
	    	$("#"+elId+" .up_scroll_area").hide();
	    	$("#"+elId+" .down_scroll_area").show();
	    }else if(st != 0 && st + ih >= sh){
	    	$("#"+elId+" .up_scroll_area").show();
	    	$("#"+elId+" .down_scroll_area").hide();
	    }else if(st == 0 && st + ih >= sh){
	    	$("#"+elId+" .up_scroll_area").hide();
	    	$("#"+elId+" .down_scroll_area").hide();
	    }
    }
	
	$(window).resize(function(){
	    scrollCheck();
    });
	
	$(".career_main").scroll(function () { 
		scrollCheck();
	});
	
	$("#btnNext").on("click",function(){
		if($("#step1").hasClass("on")){
			$("#step1").removeClass("on");
			$("#step2").addClass("on");
			$("#step1").hide();
			$("#step2").show();
			scrollCheck();
			return;
		}
		
		if($("#step2").hasClass("on")){
			location.href = "/public/last.cho";
		}
	});
	
	$("#btnPrev").on("click",function(){
		if($("#step2").hasClass("on")){
			$("#step1").addClass("on");
			$("#step2").removeClass("on");
			$("#step1").show();
			$("#step2").hide();
			scrollCheck();
			return;
		}
		
		if($("#step1").hasClass("on")){
			location.href = "/public/detail/introduction.cho";
		}
	});
	
	$("img[name=btnUp]").on("click",function(){
		var elId = "";
		$(".contents").each(function(key,val){
			if($(val).hasClass("on")){
				elId = $(val).prop("id");
			}
		});
		$("#"+elId+" .career_main").animate({scrollTop:0},500);
	});
	
	$("img[name=btnDown]").on("click",function(){
		var elId = "";
		$(".contents").each(function(key,val){
			if($(val).hasClass("on")){
				elId = $(val).prop("id");
			}
		});
		var sh = $("#"+elId+" .career_main").prop("scrollHeight");
		$("#"+elId+" .career_main").animate({scrollTop:sh},1000);
	});
	
	$(".career_img_list").on("click",function(){
		var imgPath = $(this).children("img").attr("src");
		$("#layerImg").attr("src", imgPath);
		$(".layer").show();
	});
	
	$("#layerImg").on("click",function(){
		$(".layer").hide();
	});
	
	scrollCheck();
});

</script>